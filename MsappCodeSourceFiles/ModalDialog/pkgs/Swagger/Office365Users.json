        "description": "Retrieves the trending documents for the signed in user",
        "operationId": "MyTrendingDocuments",
        "parameters": [
          {
            "in": "path",
            "name": "connectionId",
            "required": true,
            "type": "string",
            "x-ms-visibility": "internal"
          },
          {
